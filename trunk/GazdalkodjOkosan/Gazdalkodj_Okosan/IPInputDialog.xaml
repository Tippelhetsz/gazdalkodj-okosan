<Window x:Class="Gazdalkodj_Okosan.IPInputDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Név és Cim" Height="183" Width="343" ShowInTaskbar="False" ResizeMode="NoResize" WindowStyle="ToolWindow" WindowStartupLocation="CenterOwner">
    <Grid>
        <Grid Height="124" HorizontalAlignment="Left" Margin="12,12,0,0" Name="grid1" VerticalAlignment="Top" Width="300">
            <Label Content="Név:" Height="28" HorizontalAlignment="Left" Margin="6,-3,0,0" Name="label1" VerticalAlignment="Top" />
            <Label Content="Szerver IP:" Height="28" HorizontalAlignment="Left" Margin="6,60,0,0" Name="label2" VerticalAlignment="Top" />
            <TextBox Height="23" HorizontalAlignment="Left" Margin="6,31,0,0" Name="textBox1" VerticalAlignment="Top" Width="164" />
            <TextBox Height="23" HorizontalAlignment="Left" Margin="6,94,0,0" Name="textBox2" VerticalAlignment="Top" Width="164" TextChanged="textBox2_TextChanged" KeyDown="textBox2_KeyDown" />
            <Button Content="OK" Height="23" HorizontalAlignment="Left" Margin="213,94,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" IsEnabled="False" />
        </Grid>
    </Grid>
</Window>
